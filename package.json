{
    "name": "cp-blocks",
    "version": "1.0.0",
    "description": "",
    "main": "editor.js",
    "scripts": {
        "checkFormat": "npx prettier --check \"**/*.{js,json,css,scss}\" ",
        "format": "npx prettier --write \"**/*.{js,json,css,scss}\" ",
        "lint": "eslint src",
        "start": "webpack --mode=development --watch",
        "build": "npm run lint && webpack --mode=production && gulp bundle"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "linters": {
            "src/**/*.js": "eslint",
            "**/*.{js,json,css,scss": ["prettier --write", "git add"]
        }
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@wordpress/blocks": "^6.24.2",
        "@wordpress/i18n": "^3.16.0",
        "jquery": "^3.5.1",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-react": "^7.0.0",
        "@wordpress/browserslist-config": "^2.3.0",
        "autoprefixer": "^9.5.1",
        "babel-loader": "^8.0.5",
        "browserslist": "^4.5.4",
        "clean-webpack-plugin": "^2.0.1",
        "css-loader": "^2.1.1",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^7.0.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-react": "^7.21.5",
        "gulp": "^4.0.2",
        "gulp-zip": "^5.0.2",
        "husky": "^4.3.6",
        "mini-css-extract-plugin": "^0.6.0",
        "node-sass": "^4.11.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.2.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.0"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ]
}
